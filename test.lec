
""" Test Comentary """

Class Test
( @value )
{

	@Test factorial:@n {
		(@n > 1) 
		? { @n * (@Test factorial:(@n - 1)); }
		: 1;
	}

    @Test doWithParam:@param {
        @param println;
        
        (17 toString) println;
        
        (@Boolean true); 
    }
    

    
    @Test test {
		
		((@Test factorial: 12) toString) println;

    }
    
    
}

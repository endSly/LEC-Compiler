
""" Test Comentary """

Class Test
( @value )
{
    @Test doWithParam:@param {
        ((@param class) className) println;
        
        (17 toString) println;
        
        (@Boolean true); """ This is a return """
        
       "This should not be printed" println;
    }
    
    @Test do {
        (@String className) println;
        
        ((1.2 class) className) println;
        
        "[Debug] calling to @Test doWithParam;" println;
        @Test doWithParam: "Test";
        
        "[Debug] returned from @Test doWithParam;" println;
    }
    
    @Test test {
        ("Hello world! " println) println;
        ("Hola " + "mundo!") println;
        
        "[Debug] calling to @Test do;" println;
        @Test do;
        "[Debug] returning from @Test do;" println;
        
        ("'Test' length:" + (("Test" length) toString)) println;
        
        (({ 
            "Hola Routine" println;
        } class) className) println;
    }
    
    
}

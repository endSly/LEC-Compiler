

Class List
| @firstNode @lastNode |
{
    @List new {
        @self = (@super new);
        
        (@self isNotNull) ? {
            @lastNode = (@firstNode = nil);
        };
        
        
        
        ^ @self;
    }
    
    @self at: @objectIndex {
        @returnNode = @firstNode;
        
        { ^ ((@objectIndex --) > 0); } doWhileTrue: {
            @returnNode = (@returnNode nextNode);
        };
        
        
        ^ @returnNode;
    }
    
    @self [ @objectIndex ] {
        ^ (@self at: @objectIndex);
    }
    
}

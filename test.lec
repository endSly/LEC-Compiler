
""" Test Comentary """

Class Test
( @value )
{

    @Test test {
		
		"""((@Test factorial:16) toString) println;"""
		
		
		
		((@Test fibonacci:20) toString) println;
		
		((2 * 5 + 3 - 21 / 7 - 20 // 7) toString) println;

    }

	@Test factorial:@n {
		@n > 1
		? { @n * (@Test factorial:(@n - 1)) }
		: 1;
	}

	@Test fibonacci:@n {
		@n > 1 
		? { (@Test fibonacci:@n - 1) + (@Test fibonacci:@n - 2) }
		: @n
	}
    
    

    
    
}

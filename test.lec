
""" Test Comentary """

Class Test
( @value )
{

    @Test test {
		
		
		
		@self = (@Test new);

        ((@self class) className) println;

        ((@self factorial:16) toString) println;
		
		((@Test fibonacci:20) toString) println;
		
		((2 * 5 + 3 - 21 / 7 - 20 // 7) toString) println;

    }

	@self factorial:@n {
		@n > 1
		? { @n * (@self factorial:@n - 1) }
		: 1;
	}

	@Test fibonacci:@n {
		@n > 1 
		? { (@Test fibonacci:@n - 1) + (@Test fibonacci:@n - 2) }
		: @n
	}
    
    

    
    
}
